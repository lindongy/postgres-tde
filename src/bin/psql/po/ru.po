msgstr "  \\dE[S+] [МАСКА]        список сторонних таблиц\n"

#: help.c:237
#, c-format
msgid "  \\dx[+]  [PATTERN]      list extensions\n"
msgstr "  \\dx[+]  [МАСКА]        список расширений\n"

#: help.c:238
#, c-format
msgid "  \\dy     [PATTERN]      list event triggers\n"
